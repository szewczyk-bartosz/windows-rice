{
  "$schema": "https://github.com/glzr-io/zebar/raw/v2.4.0/resources/widget-schema.json",
  "htmlPath": "./bsBar.html",
  "zOrder": "normal",
  "shownInTaskbar": false,
  "focused": false,
  "resizable": false,
  "transparent": true,
  "presets": [
    {
      "name": "default",
      "anchor": "bottom_left",
      "offsetX": "10px",
      "offsetY": "-2px",
      "width": "99%",
      "height": "40px",
      "monitorSelection": {
        "type": "all"
      }
    }
  ]
}
